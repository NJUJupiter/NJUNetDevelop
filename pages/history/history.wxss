  margin: 5px;
}
.outBox{
  display: flex;
  flex-direction: row;
  margin: 5px;
  font-size: 35rpx;
  /* justify-content: center; */
}
.box{
  /* border: 2px solid black; */
  margin: 6px;
  border-radius: 20rpx;
}
.inner{
  display: flex;
  flex-direction: row;
}
.inner1{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 30rpx;
  color: grey;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  margin:5px;
}
.inner2{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 25rpx;
  color: grey;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  margin:5px;
}
.inner3{
  display: flex;
  flex-direction: row;
  margin: 5px;
  font-size: 20rpx;
  color: gray;
